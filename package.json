{
  "name": "lets-relax",
  "version": "1.0.0",
  "description": "Relaxer Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist:mac": "electron-builder -m",
    "dist:win64": "electron-builder -w --x64",
    "dist:win32": "electron-builder -w --ia32",
    "dist:linux64": "electron-builder -l --x64",
    "dist:linux32": "electron-builder -l --ia32",
    "dist:64" : "electron-builder -mwl --x64",
    "dist:32" : "electron-builder -wl --ia32"
  },
  "author": "M. Sara Friis",
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.13.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "prettier": "^2.1.2"
  }
}
